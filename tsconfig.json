{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "jsx": "react-jsx",
    "paths": {
      "src/*": ["src/*"],
      "utils": ["src/utils/index.ts"],
      "utils/*": ["src/utils/*"],
      "svg/*": ["assets/svg/*"],
      "sounds": ["assets/sounds/index.ts"],
      "sounds/*": ["assets/sounds/*"],
      "modules/*": ["src/modules/*"],
    },
    "typeRoots": ["./src/types", "./node_modules/@types"]
  }
}

